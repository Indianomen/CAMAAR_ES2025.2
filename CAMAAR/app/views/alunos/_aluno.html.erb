<div id="<%= dom_id aluno %>">
  <p>
    <strong>Nome:</strong>
    <%= aluno.nome %>
  </p>

  <p>
    <strong>Curso:</strong>
    <%= aluno.curso %>
  </p>

  <p>
    <strong>Matricula:</strong>
    <%= aluno.matricula %>
  </p>

  <p>
    <strong>Departamento:</strong>
    <%= aluno.departamento %>
  </p>

  <p>
    <strong>Formacao:</strong>
    <%= aluno.formacao %>
